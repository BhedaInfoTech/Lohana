<div class="row">
	<div class="col-md-2">
		<div class="form-group">
			<label>Flight Name</label><input type="text" id="txtflightName" class="form-control input-sm" name="Hotel.flightName" placeholder="Enter Flight Name">
		</div>
	</div>
	<div class="col-md-2">
		<div class="form-group">
			<label>Ticket Type</label><select id="drpticketType" class="form-control input-sm" name="Hotel.ticketType">
				<option>-Select Ticket Type-</option>
				<option>Business</option>
				<option>First</option>
				<option>Economy</option>
			</select>
		</div>
	</div>
	<div class="col-md-2">
		<div class="form-group">
			<label>Source </label>
			<input type="text" id="txtsource" class="form-control input-sm" name="Hotel.leavingFrom" placeholder="Enter Source ">
		</div>
	</div>
	<div class="col-md-2">
		<div class="form-group">
			<label>Destination</label><input type="text" id="txtgoingTo" class="form-control input-sm" name="Hotel.goingTo" placeholder="Enter Destination">
		</div>
	</div>

	<div class="col-md-2">
		<div class="form-group">
			<label>Is Round Trip</label><select id="drpticketType" class="form-control input-sm" name="Hotel.ticketType">
				<option>-Select-</option>
				<option>Yes</option>
				<option>No</option>
			</select>
		</div>
	</div>
</div>

<div class="row">
	<div class="col-md-2">
		<div class="form-group">
			<label>Departure Date/Time.</label><input type="text" id="dtpdepartureDateWithTime" class="form-control input-sm datepicker" name="Hotel.departureDateWithTime" placeholder="Departure Date / Time">
		</div>
	</div>
	<div class="col-md-2">
		<div class="form-group">
			<label>Arrival Date/Time.</label><input type="text" id="dtparrivalDateWithTime" class="form-control input-sm datepicker" name="Hotel.arrivalDateWithTime" placeholder="Arrival Date / Time">
		</div>
	</div>

	<div class="col-md-2">
		<div class="form-group">
			<label>Adults Count</label><input type="text" id="txtAdultCount" class="form-control input-sm" name="Hotel.adultCount" placeholder="Enter adult count">
		</div>
	</div>
	<div class="col-md-2">
		<div class="form-group">
			<label>Children Count</label><input type="text" id="txtChildrenCount" class="form-control input-sm" name="Hotel.ChildrenCount" placeholder="Enter Children count">
		</div>
	</div>

	<div class="col-md-2">
		<div class="form-group">
			<label>Budget</label>
			<input type="text" id="txtbudget" class="form-control input-sm" name="Hotel.budget" placeholder="Enter Budget">
		</div>
	</div>
</div>

